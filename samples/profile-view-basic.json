{
  "type": "object",
  "properties": {
    "did": {
      "type": "string",
      "required": true,
      "format": "did"
    },
    "handle": {
      "type": "string",
      "required": true,
      "format": "handle"
    },
    "displayName": {
      "type": "string",
      "maxGraphemes": 64,
      "maxLength": 640
    },
    "pronouns": {
      "type": "string"
    },
    "avatar": {
      "type": "string",
      "format": "uri"
    },
    "associated": {
      "type": "ref",
      "ref": "#profileAssociated"
    },
    "viewer": {
      "type": "ref",
      "ref": "#viewerState"
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "ref",
        "ref": "com.atproto.label.defs#label"
      }
    },
    "createdAt": {
      "type": "string",
      "format": "datetime"
    },
    "verification": {
      "type": "ref",
      "ref": "#verificationState"
    },
    "status": {
      "type": "ref",
      "ref": "#statusView"
    }
  },
  "required": [
    "did",
    "handle"
  ]
}
